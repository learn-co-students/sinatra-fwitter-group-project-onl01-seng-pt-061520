<!Doctype html>
<% @tweets.each do |tweet| %> 
<% user = User.find(tweet.user_id) %>
<%= user.username %>
<br>
<%= tweet.content %>
<% if user == @user %>
<a href= '/tweets/edit/<%= tweet.id %>'>Edit This Tweet</a>
<br>
<a href='/tweets/delete/'
<br>
<% end %>
<% end %>

<form action="/addition" method="POST">
  <input type="submit" value="Write a new Tweet" id="Submit" name="choice"/>
</form>