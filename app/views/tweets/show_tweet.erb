<!Doctype html>
<% @tweets.each do |tweet| %> 
<% user = User.find(tweet.user_id) %>
<%= user.username %>
<br>
<%= tweet.content %>
<% if user == @user %>
<a href= '/tweets/:id/edit/<%= tweet.id %>'>Edit This Tweet</a>
<br>
<a href='/tweets/delete/<%= tweet.id %>'>Delete This Tweet</a>
<br>
<% end %>
<% end %>

<form action="tweets/new" method="GET">
  <input type="submit" value="Write a new Tweet" id="Submit" name="choice"/>
  
</form>
<br><br><br><br>
<a href="/logout">Logout?</a>
