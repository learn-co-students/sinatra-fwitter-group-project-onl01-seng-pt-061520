<!Doctype html>
<!--<% binding.pry %>-->
<% @tweets.each do |tweet| %> 
<% user = User.find(tweet.user_id) %>
<%= "#{user.username}:" %>
<%= tweet.content %>
<br><br>


<% if user.id == @user.id %>
<a href= '/tweets/<%= tweet.id %>/edit/'>Edit This Tweet</a>
<br>
<a href='/delete/<%= tweet.id %>'>Delete This Tweet</a>
<br>
<% end %>
<% end %>

<form action="tweets/new" method="GET">
  <input type="submit" value="Write a new Tweet" id="Submit" name="choice"/>
  
</form>
<br><br><br><br>
<a href="/logout">Logout?</a>
